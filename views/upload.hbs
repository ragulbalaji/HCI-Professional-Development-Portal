<h1>{{title}}</h1>

<form action="/upload" method="post" enctype="multipart/form-data">
    <div>
        <label>File:</label>
        <input type="file" name="file"/>
    </div>
    <div>
        <label>Stuff:</label>
        <input type="text" name="text"/>
    </div>
    <div>
        <input type="submit" value="Submit"/>
    </div>
</form>
